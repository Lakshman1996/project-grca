            "children": [
                {
                    "path": "entity-selection",
                    "component": "selectentityComponent"
                },
                {
                    "path": "risk-general",
                    "component": "riskgeneralComponent"
                },
                {
                    "path": "risk-cause",
                    "component": "riskcauseComponent"
                },
                {
                    "path": "risk-roles",
